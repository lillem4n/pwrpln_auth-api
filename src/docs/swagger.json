                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Get account",
                "operationId": "get-account-by-id",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Account ID",
                        "name": "id",
